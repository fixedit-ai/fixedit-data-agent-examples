# Test Starlark inlining
[[inputs.cpu]]
  percpu = false
  totalcpu = true

[[processors.starlark]]
  source = '''
def apply(metric):
    metric.tags["processed"] = "true"
    return metric

'''

[[outputs.file]]
  files = ["stdout"]
